    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "react": "^19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "19.0.0",
    "react-icons": "^5.1.0",
    "react-live": "^4.1.8",
    "tailwindcss": "^3.4.1",
    "three": "^0.174.0",
    "uuid": "^11.1.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "zustand": "^5.0.3"
