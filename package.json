  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "\\.venv($|/)",
          "venv($|/)",
          "\\.git($|/)",
          "node_modules($|/)"
        ],
        "asar": {
          "unpack": "**/*.{node,dll}"
        },
        "icon": "loco-icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
